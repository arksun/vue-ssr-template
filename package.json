{
  "name": "vue-ssr-template",
  "description": "Vue SSR Template",
  "author": "w10036w<w10036w@gmail.com>",
  "private": true,
  "license": "Apache2.0",
  "scripts": {
    "dev": "cross-env node server",
    "beta": "cross-env DATA_ENV=beta node server",
    "prod": "cross-env DATA_ENV=prod node server",
    "start": "cross-env NODE_ENV=production DATA_ENV=prod node server",
    "start:beta": "cross-env NODE_ENV=production DATA_ENV=beta node server",
    "start:dev": "cross-env NODE_ENV=production DATA_ENV=dev node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:beta": "rimraf dist && DATA_ENV=beta npm run build:client && DATA_ENV=beta npm run build:server",
    "build:prod": "rimraf dist && DATA_ENV=prod npm run build:client && DATA_ENV=prod npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "cross-env": "^4.0.0",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "log4js": "^2.3.3",
    "lru-cache": "^4.0.2",
    "serve-favicon": "^2.4.1",
    "vue": "^2.3.2",
    "vue-router": "^2.5.0",
    "vue-server-renderer": "^2.3.2",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "axios": "^0.16.2",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-node8": "^1.2.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.1",
    "html-loader": "^0.5.1",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.2",
    "lodash": "^4.17.4",
    "mobile-detect": "^1.3.6",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.12",
    "rimraf": "^2.6.1",
    "selenium-server": "^3.0.1",
    "semver": "^5.3.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "slm": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "^0.10.1",
    "url-loader": "^0.5.8",
    "vue-lazyload": "^1.0.6",
    "vue-loader": "^12.0.2",
    "vue-style-loader": "^3.0.0",
    "vue-template-compiler": "^2.3.2",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.0.0",
    "webpack-node-externals": "^1.5.4"
  }
}
